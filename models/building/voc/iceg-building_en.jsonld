  "classes": [
    {
      "@id": "http://vocab.belgif.be/ns/building#Building",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "An enclosed and/or covered structure, above and/or below ground, intended either for the shelter of persons, animals or things, or for the production of economic goods or the provision of services, and refers to any structure permanently constructed or erected on its site."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingGeometry",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "This data types includes the geometry of the building and metadata information about which element of the building was captured and how."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Data type grouping the 3D geometry of a building or building part and the metadata information attached to this geometry."
      },
      "usage": {
        "en": "More information about LoD can be found via this link: https://docs.ogc.org/is/20-010/20-010.html#toc0"
      },
      "parents": [
        "http://vocab.belgif.be/ns/building#BuildingGeometry"
      ]
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD1",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Data type grouping the specific metadata attached to the 3D geometry, when provided by a LoD 1 representation."
      },
      "parents": [
        "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD"
      ]
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD2.horizontalGeometryReference",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Data type grouping the specific metadata attached to the 3D geometry, when provided by a LoD2 representation."
      },
      "parents": [
        "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD"
      ]
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD3",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Data type grouping the specific metadata attached to the 3D geometry, when provided by a LoD3 representation."
      },
      "parents": [
        "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD"
      ]
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD4",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Data type grouping the specific metadata attached to the 3D geometry, when provided by a LoD4 representation."
      },
      "parents": [
        "http://vocab.belgif.be/ns/building#BuildingGeometry3DLoD"
      ]
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#BuildingUnit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "The smallest unit within a building that is suitable and adapted for residential, commercial or recreational purposes and which is accessible from the outside or from a common area. A building unit is atomic, functionally independent, and may be separately sold, rented out, inherited, etc. In addition, a building unit can also be a common part."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#CadastralParcel",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "A portion of Belgian territory, geographically demarcated and identified by the AAPD on the cadastral plot plan, corresponding to the land area of one or more cadastral patrimonial plots."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#CurrentUse",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "This data type enables to detail the current use(s)."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#DateOfEvent",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "This data type includes the different possible ways to define the date of an event."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#Elevation",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Vertically-constrained dimensional property consisting of an absolute measure referenced to a well-defined surface which is commonly taken as origin (geoïd, water level, etc.)."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#EntranceLocation",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "The location where the building (unit) can be entered or exited."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#ExternalReference",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Reference to an external information system containing any piece of information related to the spatial object."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#GeographicalName",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Proper noun applied to a real world entity."
      },
      "usage": {
        "en": "This can be substituted by LangString"
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#HeightAboveGround",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Vertical distance (measured or estimated) between a low reference and a high reference."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#ICEG-Unit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "A representation of a unit."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#InteriorInstallation",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "An internal construction (generally or small size) or an internal device, serving the building."
      },
      "usage": {
        "en": "Example: stairs, lift"
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#LangCode",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "The official code of the language."
      },
      "parents": []
    },
    {
      "@id": "http://vocab.belgif.be/ns/building#StructuredIdentificator",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "definition": {
        "en": "Identifier of an object broken down into its parts."
      },
      "parents": []
    }
